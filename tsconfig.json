{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/assets/*": [
        "assets/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/models/*": [
        "models/*"
      ],
      "@/navigator/*": [
        "navigator/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
    },
    "types": [
      "jest",
      "node",
      "@types/styled-components-react-native"
    ]
  }
}